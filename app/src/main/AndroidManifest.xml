<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wallymisr.flowersapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/flower"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".RegisterationActivity" />
        <activity android:name=".ContactUsActivity" />
        <activity android:name=".CountryActivity" />
        <activity android:name=".GovernorateActivity" />
        <activity android:name=".AreaActivity" />
        <activity android:name=".ChoiceActivity" />
        <activity android:name=".WeddingshopActivity" />
        <activity android:name=".NormalshopActivity" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".PaymentresultActivity" />
        <activity android:name=".ConfirmationActivity"></activity>
    </application>

</manifest>